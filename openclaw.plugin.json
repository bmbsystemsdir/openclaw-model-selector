{
  "id": "openclaw-model-selector",
  "kind": "extension",
  "version": "4.0.0",
  "uiHints": {
    "enabled": {
      "label": "Enabled",
      "help": "Enable/disable automatic model selection"
    },
    "defaultModel": {
      "label": "Default Model",
      "placeholder": "gemini-flash",
      "help": "The default model for all conversations"
    },
    "collaborationChannel": {
      "label": "Collaboration Channel ID",
      "help": "Discord channel ID where collaboration auto-switch is enabled"
    },
    "collaborationAutoSwitch": {
      "label": "Collaboration Auto-Switch",
      "help": "Automatically switch to complement model in collaboration channel"
    },
    "agentId": {
      "label": "Agent ID",
      "help": "This agent's identifier (to avoid detecting own model announcements)"
    }
  },
  "configSchema": {
    "type": "object",
    "additionalProperties": true,
    "properties": {
      "enabled": { "type": "boolean" },
      "defaultModel": { "type": "string" },
      "collaborationChannel": { "type": "string" },
      "collaborationAutoSwitch": { "type": "boolean" },
      "agentId": { "type": "string" },
      "modelComplements": {
        "type": "object",
        "additionalProperties": { "type": "string" }
      },
      "models": {
        "type": "object",
        "additionalProperties": true,
        "properties": {
          "simple": {
            "oneOf": [
              { "type": "string" },
              { "type": "array", "items": { "type": "string" } }
            ]
          },
          "planning": {
            "oneOf": [
              { "type": "string" },
              { "type": "array", "items": { "type": "string" } }
            ]
          },
          "complex": {
            "oneOf": [
              { "type": "string" },
              { "type": "array", "items": { "type": "string" } }
            ]
          },
          "coding": {
            "oneOf": [
              { "type": "string" },
              { "type": "array", "items": { "type": "string" } }
            ]
          }
        }
      },
      "approvalTriggers": {
        "type": "array",
        "items": { "type": "string" }
      },
      "overrideTriggers": {
        "type": "array",
        "items": { "type": "string" }
      }
    }
  }
}
